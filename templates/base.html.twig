<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>{% block title %}Welcome!{% endblock %}</title>
<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
{% block stylesheets %}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css">
<link href="/bundles/siganushkamedia/all.css" rel="stylesheet">
<link href="/bundles/siganushkaproduct/all.css" rel="stylesheet">
{% endblock %}
</head>
<body>
<div class="container-fluid my-4">
  <div class="row">
    <div class="col">
      <ul class="dropdown-menu position-static d-grid gap-1 p-2 rounded-3 mx-0 w-220px" data-bs-theme="light">
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_index_index') }}">Welcome</a>
        </li>
        <li><hr class="dropdown-divider"></li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_option_index') }}">选项</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_option_optiontype') }}">OptionType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_option_optiontype') }}">OptionValueType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_index') }}">产品</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_producttype') }}">ProductType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productvariantcollectiontype') }}">ProductVariantCollectionType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_product_productvarianttype') }}">ProductVariantType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_variant_index') }}">库存</a>
        </li>
        <li><hr class="dropdown-divider"></li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_index') }}">订单</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_ordertype') }}">OrderType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_order_orderitemtype') }}">OrderItemType</a>
        </li>
        <li><hr class="dropdown-divider"></li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_index') }}">文件</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediauploadtype') }}">MediaUploadType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediatype') }}">MediaType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediachanneltype') }}">MediaChannelType</a>
        </li>
        <li>
          <a class="dropdown-item rounded-2" href="{{ path('app_media_mediafiletype') }}">MediaFileType</a>
        </li>
      </ul>
    </div>
    <div class="col-9">
      {% for label, messages in app.flashes %}
        {% for message in messages %}
          <div class="alert alert-{{ label }}">{{ message }}</div>
        {% endfor %}
      {% endfor %}
      {% block body %}{% endblock %}
    </div>
  </div>
</div>

{% block javascripts %}
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
<script src="/bundles/siganushkamedia/all.js"></script>
<script src="/bundles/siganushkaproduct/all.js"></script>
{% endblock %}
</body>
</html>
